{% extends '_base.html.twig' %}

{% block pageTitle %}Add User Page {% endblock %}

{% block main %}
    <form action="/addUser" method="post">

        <p>
            Username:
            <input type="text" name="username">
        </p>

        <p>
            Password:
            <input type="password" name="password">
        </p>

        <input type="submit" value="Add Administrator">

    </form>

{% endblock %}